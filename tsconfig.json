  "compilerOptions": {
    "rootDir": "./src",
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "isolatedModules": true,
    "strict": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "lib": ["ES2022"]
  }
}
